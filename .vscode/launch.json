{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug board-request-service with Nx",
      "runtimeExecutable": "npx",
      "runtimeArgs": ["nx", "serve", "board-request-service"],
      "env": {
        "NODE_OPTIONS": "--inspect=9229"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/board-request-service/dist/**/*.(m|c|)js",
        "!**/node_modules/**"
      ]
    },
